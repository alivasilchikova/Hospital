<Application x:Class="HospitalApp.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:HospitalApp"
             xmlns:viewModels="clr-namespace:HospitalApp.Data"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <viewModels:ViewModel x:Key="ViewModel"/>
        <viewModels:ViewModelDoctor x:Key="ViewModelDoctor"/>
        <viewModels:ViewModelPosition x:Key="ViewModelPosition"/>
        <viewModels:ViewModelAppoinment x:Key="ViewModelAppoinment"/>
        <viewModels:ViewModelAddPosition x:Key="ViewModelAddPosition"/>
        <viewModels:ViewModelService x:Key="ViewModelService"/>
    </Application.Resources>
</Application>
